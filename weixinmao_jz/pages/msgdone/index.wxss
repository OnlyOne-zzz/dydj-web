page {
    background-color: #f0f0f0;
}

.container {
    background-color: #f0f0f0;
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.addressimg {
    width: 100%;
    height: 2px;
    background: #fff;
}

.addressimg image {
    width: 100%;
    height: 2px;
    float: left;
}
.userInfo{
    width: 100%;
    box-sizing: border-box;
    background-color: #fff;
    padding-left: 40px;
    padding-top: 10px;
}

.userInfo > view {
    width: 100%;
    height: 40px;
    border-bottom: 1px solid #e5e5e5;
    position: relative;
}
.userInfo > view >image{
    display: inline-block;
    height: 23px;
    width: 23px;
    float: left;
    margin-top: 10px;
    margin-left: -30px;
}
.listcontent{
    width: 100%;
    height: 40px;
    line-height: 40px;
    font-size: .8rem;
    display: flex;
    justify-content:space-between;
}
.listTime{
    padding-right: 10px;
}
.right{
    width: 15px !important;
    height: 15px !important;
    position: absolute;
    top: 50%;
    right: 10px;
    transform: translateY(-50%);
    margin: 0 !important;
    padding: 0 !important;
    clear: left !important;
}
.userInfo > view >input{
    font-size: .8rem;
    display: inline-block;
    height: 100%;
    width: calc(100% - 10px);
    padding-left: 0px;
}
.newaddress {
    width: 750rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    background: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    border-radius: 10rpx;
}

.newaddress .showaddress {
    display: flex;
    flex-direction: column;
    padding-left: 20rpx;
}

.newaddress .showaddress view:nth-child(1) {
    font-weight: 600;
    font-size: .9rem;
}

.newaddress .showaddress view:nth-child(2) {
    font-size: .7rem;
}

.newaddress image {
    width: 45rpx;
    height: 45rpx;
    margin-top: 10rpx;
}

.newaddress2 {
    width: 750rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    background: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    border-radius: 10rpx;
}

.newaddress2 .getaddress {
    margin-right: 50rpx;
}

.flexaddress {
    display: flex;
    flex-direction: row;
}

.flexaddress image {
    width: 45rpx;
    height: 45rpx;
    margin-top: 5rpx;
}

.container .main {
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #FFF;
    align-self: center;
    padding: 10rpx;
    border-radius: 10rpx;
    margin-top: 0rpx;
    margin-bottom: 10rpx;
}

.container .main .title {
    width: 700rpx;
    padding-top: 15rpx;
    padding-bottom: 15rpx;
    font-size: 1rem;
    color: #000;
    border-bottom: 1rpx dotted #f5f5f5;
    font-weight: 600;
    margin-left: 15rpx;
}

.container .main .list {
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #FFF;
    padding: 20rpx;
}

.container .main .list .item {
    width: 750rpx;
    border-bottom: 1rpx solid #f5f5f5;
    align-self: center;
    padding-top: 25rpx;
    padding-bottom: 25rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.container .main .list .item .name {
    width: 35%;
    font-size: .9rem;
    color: #2d2d2d;
}

.container .main .list .item .selectitem {
    width: 750rpx;
    display: flex;
