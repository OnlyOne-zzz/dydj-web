page{
    background-color: #f0f0f0;
}
.container {
    min-height: 100%;
    padding-top: 5px;
}

.header {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    padding-top: 40rpx;
    padding-bottom: 40rpx;
}

.header .search {
    width: 650rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 5rpx;
    background: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.header .search view {
    color: #a6a6a6;
    font-size: .7rem;
    align-self: center;
}

.header .search icon {
    margin-right: 30rpx;
    margin-top: 18rpx;
}

.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
    height: 362.5rpx;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 362.5rpx;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-radius: 50%;
    transition: all .6s;
    opacity: .5;
}

.swiper-container .dots .dot.active {
    width: 14rpx;
    opacity: 1;
}

.weui-grids {
    position: relative;
    overflow: hidden;
    border: none;
}

.weui-grids:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 0.1rpx;
    border-top: 0rpx solid #ebebeb;
    color: #D9D9D9;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}

.weui-grids:after {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 0.1rpx;
    bottom: 0;
    border-left: 0rpx solid #ebebeb;
    color: #D9D9D9;
    transform-origin: 0 0;
    transform: scaleX(0.5);
}

.weui-gridss {
    background-color: #fff;
    position: relative;
    width: 96%;
    height: 120px;
    box-sizing: border-box;
    border: none;
    padding: 10px;
    margin: auto;
    margin-bottom: 5px;
    border-radius: 8px;
}

