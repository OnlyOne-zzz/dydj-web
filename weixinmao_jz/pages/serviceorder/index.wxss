.weui-grid__icon {
    width: 98px;
    height: 98px;
    float: left;
    border-radius: 5px;
    overflow: hidden;
    border: 1px solid #f0f0f0;
}

.weui-grid__icon image {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.weui-grid__icon+.weui-grid__label {
    margin-top: 10rpx;
}

.weui-gridlabel {
    width: calc(100% - 100px);
    height: 100%;
    float: right;
    font-size: .8rem;
    overflow: hidden;
    box-sizing: border-box;
    margin: 0px;
    padding-left: 10px;
    color: #222;
    position: relative;
}
.title{
    width: 100%;
    font-size: .8rem;
    display: flex;
    justify-content: space-between
}
.title>text:nth-of-type(1){
    font-weight: bold;
    font-weight: .9rem;
}
.title>text:nth-of-type(2){
    font-size: .7rem;
}
.desc{
    font-size: .6rem;
}
.desc > text{
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.desc2{
    font-size: .6rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.time{
    position: absolute;
    left: 10px;
    bottom: 0px;
}
.notice {
    width: 750rpx;
    background: #FFF;
    display: flex;
    flex-direction: row;
    font-size: .8rem;
}

.notice text {
    color: #42BE9C;
    margin-top: 10rpx;
    margin-right: 10rpx;
}

.fjyy {
    width: 115px;
}
.fjyy > image{
    width: 100%;
    /* height: 30px; */
}

.swiper_container_move {
    height: 55rpx;
    line-height: 55rpx;
    width: 100%;
    padding: 0px 20px;
    padding-left: 10px;
}

.swiper_item_move > image{
    width: 20px;
    height: 20px;
    vertical-align: -5px;
    margin-right: 5px;
    border-radius: 50%;
}

.swiper_item_move {
    font-size: 25rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    letter-spacing: 2px;
}