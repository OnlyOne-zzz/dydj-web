@import "../../resource/wxParse/wxParse.wxss";

.container {
    background-color: #FFF;
    min-height: 100%;
    display: flex;
    flex-direction: column;
}

.container .title1 {
    display: flex;
    flex-direction: column;
    width: 750rpx;
    border-bottom: 1px dotted #ccc;
    align-items: center;
}

.container .title1 view {
}

.container .title1 view:nth-child(1) {
    font-size: .9rem;
    font-weight: bold;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.container .content {
    align-self: center;
    padding-left: 2%;
    padding-right: 2%;
}
@import "../../resource/wxParse/wxParse.wxss";

image {
    width: 100% !important;
   }
.btnGroup {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 140rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 50;
}
    button {
     height: 88rpx;
     line-height: 88rpx;
     border-radius: 44rpx;
     font-size: 28rpx;
     font-weight: normal;
    }
     .leftBtn {
      width: 238rpx;
      border: 2rpx solid #ff7635;
      color: #ff7635;
     }
     .rightBtn {
      width: 412rpx;
      background-color: #ff7635;
      color: #fff;
      background-color: #cccccc !important;
     }
     .title {
        margin-left: 40rpx;
        margin-top: 40rpx;
        text-align: center;
        width: fit-content;
        width: -webkit-fit-content;
        width: -moz-fit-content;
        padding: 10rpx 30rpx;
        background: rgba(0, 0, 0, 1);
        border-radius: 10rpx;
        font-size: 30rpx;
        font-family: Adobe Heiti Std;
        font-weight: normal;
        color: rgba(254, 254, 254, 1);
       }

     .txt {
      text-align: justify;
      font-size: 30rpx;
      font-family: PingFangSC;
      font-weight: normal;
      color: rgba(51, 51, 51, 1);
      line-height: 40rpx;
      text-indent: 2em;
      padding: 10rpx 30rpx;
     }
    
   
   button[disabled]:not([type]) {
    background-color: #cccccc !important;
    color: #ffffff !important;
   }